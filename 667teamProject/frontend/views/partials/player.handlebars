  <div class='container-fluid'>
    <div>PLAYER #1</div>
    <div>
      <ul>
        {{#each card}}
        <li>
          <div class='card'>
            <div class='card-body'>
              <div class='card-title'>{{this.color}}</div>
              <div class='card-text'>{{this.value}}</div>
            </div>
          </div>
        </li>
        {{/each}}
      </ul>
    </div>
  </div>